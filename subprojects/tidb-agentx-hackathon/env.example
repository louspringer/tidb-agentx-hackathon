# TiDB Serverless Configuration
TIDB_HOST=your-tidb-host.tidbcloud.com
TIDB_PORT=4000
TIDB_USER=your-username
TIDB_PASSWORD=your-password
TIDB_DATABASE=your-database

# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO
API_HOST=0.0.0.0
API_PORT=8000

# Security Configuration
SECRET_KEY=your-secret-key-here
DEBUG=false

# Database Configuration
DATABASE_URL=mysql+pymysql://${TIDB_USER}:${TIDB_PASSWORD}@${TIDB_HOST}:${TIDB_PORT}/${TIDB_DATABASE}

# Vector Search Configuration
VECTOR_DIMENSION=1536
VECTOR_TABLE=embeddings
VECTOR_INDEX_TYPE=ivf_flat

# AI Agent Configuration
MAX_AGENTS=10
MAX_WORKFLOWS=50
WORKFLOW_TIMEOUT=300

# Testing Configuration
TEST_MODE=false
MOCK_TIDB=false
TEST_DATABASE=test_database
